 <!-- Header -->
    <div class="header bg-gradient-primary py-6 py-lg-6 pt-lg-6">
      <div class="container">
        <div class="header-body">
          <div class="row align-items-center py-4">
            <div class="col-lg-6 col-5">
              <a href="#" class="btn btn-sm btn-neutral">Post new article</a>
            </div>
          </div>
        </div>
      </div>
      <div class="separator separator-bottom separator-skew zindex-100">
        <svg x="0" y="0" viewBox="0 0 2560 100" preserveAspectRatio="none" version="1.1" xmlns="http://www.w3.org/2000/svg">
          <polygon class="fill-white" points="2560 0 2560 100 0 100"></polygon>
        </svg>
      </div>
    </div>
    <!-- Page content -->
    <div class="container mt--12">
      <div class="row py-6 py-lg-6 pt-lg-6">
        {{#each articles}}
            <div class="col-xl-4 col-md-6">
              <div class="card">
                <!-- Card body -->
                <div class="card-img-250" style="background-image:url('{{this.profile_img_url}}')">
                  {{!-- <img class="card-img-top" src="{{this.profile_img_url}}" alt="{{this.article_name}}"> --}}
                </div>
                <div class="card-body">
                  <h5 class="card-title">{{this.article_name}}</h5>
                  <p class="card-text">{{this.content}}</p>
                  <a href="articles/{{this.id}}" class="btn btn-primary">Read more</a>
                </div>
              </div>
            </div>
        {{/each}}
      </div>
    </div>
